
#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
   int n,k;
   cin>>n>>k;
   int a[n];
   for(int i=0;i<n;i++){
       cin>>a[i];
       
   }
   sort(a,a+n);
   int max=INT_MIN;
   for(int i=0;i<=n-k;i++){
       if(a[i]>max){
           max=a[i];
       }
   }
   cout<<max;
}

